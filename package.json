{
  "name": "movies-fe",
  "version": "0.0.3",
  "description": "Movie and movie item tracker, front-end only.",
  "keywords": [
    "aswnn",
    "react",
    "typescript",
    "movies",
    "nextjs"
  ],
  "homepage": "https://www.asitewithnoname.com/",
  "license": "MIT",
  "author": "Brian Duffey <brian.e.duffey@gmail.com>",
  "contributors": [],
  "main": "pages/index.tsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/movies-fe"
  },
  "scripts": {
    "build": "next build",
    "clean": "rm -r .next",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev": "next dev",
    "dev:up": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "env": "sync-dotenv",
    "gql:download": "apollo client:download-schema graphql/schema.graphql --endpoint=http://localhost:4000/dev/graphql",
    "license": "license-check-and-add add -f license-check-and-add-config.json",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "lint:script": "scriptlint && npmPkgJsonLint .",
    "lint:script:fix": "scriptlint --fix",
    "prepare": "husky install",
    "prod": "next build && next start",
    "prod:up": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "setup": "npx typesync && npm i && npm prune",
    "start": "next start",
    "test": "jest",
    "test:ci": "jest --ci --coverage --reporters='default' --reporters='./github-actions-reporter'",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:watch": "jest --watch",
    "typeCheck": "tsc",
    "validate": "npm run lint && tsc --noEmit && npx ts-prune"
  },
  "config": {},
  "dependencies": {
    "@apollo/client": "3.4.16",
    "@bduff9/pro-light-svg-icons": "5.15.4",
    "@bduff9/pro-regular-svg-icons": "5.15.4",
    "@bduff9/pro-solid-svg-icons": "5.15.4",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@sentry/browser": "7.119.1",
    "@zeit/next-css": "1.0.1",
    "apollo3-cache-persist": "0.13.0",
    "bloomer": "0.6.5",
    "bulma": "0.9.3",
    "bulmaswatch": "0.8.1",
    "clsx": "1.1.1",
    "date-fns": "2.26.0",
    "formik": "2.2.9",
    "graphql": "15.7.1",
    "graphql-tag": "2.12.6",
    "isomorphic-unfetch": "3.1.0",
    "jwt-simple": "0.5.6",
    "mysql": "2.18.1",
    "next": "12.0.4",
    "next-auth": "<3.18.0",
    "next-pwa": "5.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-toastify": "<8.0.0",
    "styled-components": "5.3.3",
    "universal-cookie": "4.0.4",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/fragment-matcher": "3.2.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-document-nodes": "2.2.1",
    "@graphql-codegen/typescript-graphql-files-modules": "2.1.0",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@graphql-codegen/typescript-react-apollo": "3.2.2",
    "@types/autoprefixer": "10.2.0",
    "@types/enzyme": "3.10.10",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/eslint": "7.28.2",
    "@types/jest": "27.0.3",
    "@types/mysql": "2.15.19",
    "@types/next-auth": "3.15.0",
    "@types/node": "16.11.9",
    "@types/react": "17.0.35",
    "@types/react-dom": "17.0.11",
    "@types/sass": "1.43.0",
    "@types/styled-components": "5.1.15",
    "@types/stylelint": "13.13.3",
    "@types/universal-cookie": "3.0.0",
    "@types/yup": "0.29.13",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "@welldone-software/why-did-you-render": "6.2.3",
    "autoprefixer": "10.4.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "<8.0.0",
    "eslint-config-next": "12.0.4",
    "eslint-plugin-clean-regex": "0.5.2",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettierx": "0.18.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-you-dont-need-momentjs": "1.6.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "jest-css-modules": "2.1.0",
    "jest-transform-css": "3.0.0",
    "license-check-and-add": "4.0.3",
    "lint-staged": "11.1.2",
    "npm-package-json-lint": "5.4.2",
    "sass": "1.43.4",
    "scriptlint": "2.1.8",
    "stylefmt": "6.0.3",
    "stylelint": "14.1.0",
    "stylelint-config-standard": "24.0.0",
    "sync-dotenv": "2.6.2",
    "ts-jest": "27.0.7",
    "typescript": "4.5.2",
    "vercel": "23.1.2"
  },
  "engines": {
    "node": "14.x"
  },
  "private": true,
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "\\.(css|less|scss|sass)$": "jest-transform-css"
    },
    "testMatch": [
      "**/*.test.(ts|tsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,json,css}": [
      "prettierx --write",
      "git add"
    ],
    "**/*.scss": [
      "stylefmt",
      "stylelint --syntax scss --fix",
      "git add"
    ],
    "package.json": [
      "scriptlint --fix",
      "npmPkgJsonLint .",
      "git add"
    ],
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  },
  "browserslist": {
    "production": [
      "last 2 chrome versions",
      "last 2 and_chr versions",
      "last 2 ff versions",
      "last 2 safari versions",
      "last 2 ios versions",
      "last 2 edge versions",
      "ie >= 11"
    ],
    "development": [
      "last 2 chrome versions",
      "last 2 and_chr versions",
      "last 2 ff versions",
      "last 2 safari versions",
      "last 2 ios versions",
      "last 2 edge versions",
      "ie >= 11"
    ]
  }
}
