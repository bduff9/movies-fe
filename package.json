{
  "name": "movies-fe",
  "version": "0.0.3",
  "description": "Movie and movie item tracker, front-end only.",
  "keywords": [
    "aswnn",
    "react",
    "typescript",
    "movies",
    "nextjs"
  ],
  "homepage": "https://www.asitewithnoname.com/",
  "license": "MIT",
  "author": "Brian Duffey <brian.e.duffey@gmail.com>",
  "contributors": [],
  "main": "pages/index.tsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/movies-fe"
  },
  "scripts": {
    "build": "next build",
    "clean": "rm -r .next",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev": "next dev",
    "env": "sync-dotenv",
    "gql:download": "apollo client:download-schema graphql/schema.graphql --endpoint=http://localhost:4000/dev/graphql",
    "license": "license-check-and-add add -f license-check-and-add-config.json",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "lint:script": "scriptlint && npmPkgJsonLint .",
    "lint:script:fix": "scriptlint --fix",
    "prepare": "husky install",
    "setup": "npx typesync && npm i && npm prune",
    "start": "next start",
    "test": "jest",
    "test:ci": "jest --ci --coverage --reporters='default' --reporters='./github-actions-reporter'",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:watch": "jest --watch",
    "typeCheck": "tsc",
    "validate": "npm run lint && tsc --noEmit && npx ts-prune"
  },
  "config": {},
  "dependencies": {
    "@apollo/client": "3.3.13",
    "@bduff9/pro-light-svg-icons": "5.15.3",
    "@bduff9/pro-regular-svg-icons": "5.15.3",
    "@bduff9/pro-solid-svg-icons": "5.15.3",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-brands-svg-icons": "5.15.3",
    "@fortawesome/free-regular-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@sentry/browser": "6.2.5",
    "@zeit/next-css": "1.0.1",
    "apollo3-cache-persist": "0.9.1",
    "bloomer": "0.6.5",
    "bulma": "0.9.2",
    "bulmaswatch": "0.8.1",
    "clsx": "1.1.1",
    "date-fns": "2.19.0",
    "formik": "2.2.6",
    "graphql": "15.5.0",
    "graphql-tag": "2.11.0",
    "isomorphic-unfetch": "3.1.0",
    "jwt-simple": "0.5.6",
    "mysql": "2.18.1",
    "next": "10.1.2",
    "next-auth": "3.13.3",
    "next-pwa": "5.0.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-toastify": "7.0.3",
    "styled-components": "5.2.3",
    "universal-cookie": "4.0.4",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-document-nodes": "1.17.10",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-react-apollo": "2.2.3",
    "@types/autoprefixer": "10.2.0",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/eslint": "7.2.8",
    "@types/jest": "26.0.22",
    "@types/mysql": "2.15.18",
    "@types/next-auth": "3.13.0",
    "@types/node": "14.14.37",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/sass": "1.16.0",
    "@types/styled-components": "5.1.9",
    "@types/stylelint": "9.10.1",
    "@types/universal-cookie": "3.0.0",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "@welldone-software/why-did-you-render": "6.1.1",
    "autoprefixer": "10.2.5",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.23.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettierx": "0.17.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-you-dont-need-momentjs": "1.6.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "jest-css-modules": "2.1.0",
    "jest-transform-css": "2.1.0",
    "license-check-and-add": "4.0.2",
    "lint-staged": "10.5.4",
    "npm-package-json-lint": "5.1.0",
    "sass": "1.32.8",
    "scriptlint": "2.1.3",
    "stylefmt": "6.0.3",
    "stylelint": "13.12.0",
    "stylelint-config-standard": "21.0.0",
    "sync-dotenv": "2.6.2",
    "ts-jest": "26.5.4",
    "typescript": "4.2.3",
    "vercel": "21.3.3"
  },
  "engines": {
    "node": "14.x"
  },
  "private": true,
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "\\.(css|less|scss|sass)$": "jest-transform-css"
    },
    "testMatch": [
      "**/*.test.(ts|tsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,json,css}": [
      "prettierx --write",
      "git add"
    ],
    "**/*.scss": [
      "stylefmt",
      "stylelint --syntax scss --fix",
      "git add"
    ],
    "package.json": [
      "scriptlint --fix",
      "npmPkgJsonLint .",
      "git add"
    ],
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix",
    "*.css": "stylelint --fix"
  },
  "browserslist": {
    "production": [
      "last 2 chrome versions",
      "last 2 and_chr versions",
      "last 2 ff versions",
      "last 2 safari versions",
      "last 2 ios versions",
      "last 2 edge versions",
      "ie >= 11"
    ],
    "development": [
      "last 2 chrome versions",
      "last 2 and_chr versions",
      "last 2 ff versions",
      "last 2 safari versions",
      "last 2 ios versions",
      "last 2 edge versions",
      "ie >= 11"
    ]
  }
}
