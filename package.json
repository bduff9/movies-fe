{
  "name": "movies-fe",
  "version": "0.0.3",
  "description": "Movie and movie item tracker, front-end only.",
  "keywords": [
    "aswnn",
    "react",
    "typescript",
    "movies",
    "nextjs"
  ],
  "homepage": "https://www.asitewithnoname.com/",
  "license": "MIT",
  "author": "Brian Duffey <brian.e.duffey@gmail.com>",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/bduff9/movies-fe"
  },
  "scripts": {
    "build": "next build",
    "clean": "rm -r .next",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev": "next dev",
    "env": "sync-dotenv",
    "gql:download": "apollo client:download-schema graphql/schema.graphql --endpoint=http://localhost:4000/dev/graphql",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "lint:script": "scriptlint && npmPkgJsonLint .",
    "lint:script:fix": "scriptlint --fix",
    "setup": "npx typesync && npm i && npm prune",
    "start": "next start",
    "test": "jest",
    "test:ci": "jest --ci --reporters='default' --reporters='./github-actions-reporter'",
    "test:coverage": "jest --coverage --env=jsdom --coverage --testResultsProcessor ./node_modules/jest-sonar-reporter/index.js",
    "test:watch": "jest --watch",
    "typeCheck": "tsc",
    "validate": "npm run lint && tsc --noEmit && npx ts-prune"
  },
  "config": {},
  "dependencies": {
    "@apollo/react-hooks": "3.1.5",
    "@apollo/react-ssr": "3.1.5",
    "@bduff9/fontawesome-pro": "5.15.2",
    "@fortawesome/fontawesome-svg-core": "1.2.34",
    "@fortawesome/free-brands-svg-icons": "5.15.2",
    "@fortawesome/free-regular-svg-icons": "5.15.2",
    "@bduff9/pro-light-svg-icons": "5.15.2",
    "@bduff9/pro-regular-svg-icons": "5.15.2",
    "@bduff9/pro-solid-svg-icons": "5.15.2",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@sentry/browser": "6.1.0",
    "@zeit/next-css": "1.0.1",
    "apollo-boost": "0.4.9",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-cache-persist-dev": "0.2.1",
    "apollo-client": "2.6.10",
    "apollo-link-context": "1.0.20",
    "apollo-link-http": "1.5.17",
    "aws-amplify": "2.3.0",
    "aws-amplify-react": "4.2.23",
    "bloomer": "0.6.5",
    "bulma": "0.9.2",
    "bulmaswatch": "0.8.1",
    "clsx": "1.1.1",
    "date-fns": "2.17.0",
    "formik": "2.2.6",
    "graphql": "15.5.0",
    "graphql-tag": "2.11.0",
    "isomorphic-unfetch": "3.1.0",
    "jwk-to-pem": "2.0.4",
    "next": "10.0.6",
    "next-pwa": "5.0.5",
    "next-with-apollo": "5.1.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-toastify": "7.0.3",
    "sass": "1.32.7",
    "styled-components": "5.2.1",
    "universal-cookie": "4.0.4",
    "yup": "0.32.8"
  },
  "devDependencies": {
    "@apollo/react-testing": "3.1.4",
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-document-nodes": "1.17.9",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.1",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@types/autoprefixer": "10.2.0",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/eslint": "7.2.6",
    "@types/jest": "26.0.20",
    "@types/jwk-to-pem": "2.0.0",
    "@types/node": "14.14.28",
    "@types/node-sass": "4.11.1",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/sass": "1.16.0",
    "@types/styled-components": "5.1.7",
    "@types/stylelint": "9.10.1",
    "@types/universal-cookie": "3.0.0",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "@welldone-software/why-did-you-render": "6.0.5",
    "autoprefixer": "10.2.4",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.20.0",
    "eslint-plugin-css-modules": "2.11.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettierx": "0.17.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-you-dont-need-momentjs": "1.6.0",
    "husky": "5.0.9",
    "jest": "26.6.3",
    "jest-css-modules": "2.1.0",
    "jest-transform-css": "2.1.0",
    "lint-staged": "10.5.4",
    "npm-package-json-lint": "5.1.0",
    "scriptlint": "2.1.2",
    "stylefmt": "6.0.3",
    "stylelint": "13.10.0",
    "stylelint-config-standard": "20.0.0",
    "ts-jest": "26.5.1",
    "typescript": "4.1.5"
  },
  "engines": {
    "node": "14.x"
  },
  "private": true,
  "husky": {
    "hooks": {
      "post-merge": "npm run setup",
      "pre-commit": "lint-staged && npm run env",
      "pre-push": "npm run validate && npm run test:ci"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "\\.(css|less|scss|sass)$": "jest-transform-css"
    },
    "testMatch": [
      "**/*.test.(ts|tsx)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{md,json,css}": [
      "prettierx --write",
      "git add"
    ],
    "**/*.scss": [
      "stylefmt",
      "stylelint --syntax scss --fix",
      "git add"
    ],
    "package.json": [
      "scriptlint --fix",
      "npmPkgJsonLint .",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      "last 2 chrome versions",
      "last 2 and_chr versions",
      "last 2 ff versions",
      "last 2 safari versions",
      "last 2 ios versions",
      "last 2 edge versions",
      "ie >= 11"
    ],
    "development": [
      "last 2 chrome versions",
      "last 2 and_chr versions",
      "last 2 ff versions",
      "last 2 safari versions",
      "last 2 ios versions",
      "last 2 edge versions",
      "ie >= 11"
    ]
  }
}
